<?xml version="1.0"?>
<robot name="origins">
  
  <link name="hipMid">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://../../STL/HipMid.stl" scale="0.01"/>
      </geometry>
    </visual>
    
    <collision>
        <geometry>
            <mesh filename="package://../../STL/HipMid.stl" scale="0.01"/>
        </geometry>
    </collision>
  </link>


  <link name="hipFront">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.06"/>
      <geometry>
        <mesh filename="package://../../STL/HipFront.stl" scale="0.01"/>
      </geometry>
    </visual>
    
    <collision>
        <geometry>
            <mesh filename="package://../../STL/HipFront.stl" scale="0.01"/>
        </geometry>
    </collision>
  </link>


  <link name="hipBack">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 -0.06"/>
      <geometry>
        <mesh filename="package://../../STL/HipBack.stl" scale="0.01"/>
      </geometry>
    </visual>
    
    <collision>
        <geometry>
            <mesh filename="package://../../STL/HipBack.stl" scale="0.01"/>
        </geometry>
    </collision>
  </link>

<!-- 
  <joint name="fixedHipMid" type="fixed">
    <parent link="hipMid" />
    <child link="hipMid" />
    <origin xyz="0 0 0" rpy="0 0 0" />
  </joint> 
  -->


  <joint name="fixedHipFront" type="fixed">
    <parent link="hipMid" />
    <child link="hipFront" />
    <origin xyz="0 0 0" rpy="0 0 0" />
  </joint>


  <joint name="fixedHipBack" type="fixed">
    <parent link="hipMid" />
    <child link="hipBack" />
    <origin xyz="0 0 0" rpy="0 0 0" />
  </joint>


  <link name="ServoHolderR">
    <visual>
      <origin rpy="4.71 0 0" xyz="0.8 0.5 -0.25"/>
      <geometry>
        <mesh filename="package://../../STL/ServoHolderR.stl" scale="0.01"/>
      </geometry>
    </visual>
    
    <collision>
        <geometry>
            <mesh filename="package://../../STL/ServoHolderR.stl" scale="0.01"/>
        </geometry>
    </collision>
  </link>

  <joint name="revoluteRightHip" type="revolute">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="hipFront"/>
    <child link="ServoHolderR"/>
    <axis xyz="0 0 1"/>
    <limit lower="0" upper="${2*pi}" velocity="100" effort="1"/> 
  </joint>

  <link name="servoHolderLeft">
    <visual>
      <origin rpy="4.71 0 0" xyz="0.8 -0.5 -0.25"/>
      <geometry>
        <mesh filename="package://../../STL/ServoHolderL.stl" scale="0.01"/>
      </geometry>
    </visual>
    
    <collision>
        <geometry>
            <mesh filename="package://../../STL/ServoHolderL.stl" scale="0.01"/>
        </geometry>
    </collision>
  </link>


  <joint name="revoluteLeftHip" type="revolute">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="hipBack"/>
    <child link="servoHolderLeft"/>
    <axis xyz="0 0 1"/>
    <limit lower="0" upper="${2*pi}" velocity="100" effort="1"/>
  </joint>


  # develop a revolute joint for the up_Leg_Right
  <link name="UpperLegR">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://../../STL/UpperLegR.stl" scale="0.01"/>
      </geometry>
    </visual>
    
    <collision>
        <geometry>
            <mesh filename="package://../../STL/UpperLegR.stl" scale="0.01"/>
        </geometry>
    </collision>
  </link>


  <joint name="revoluteUpperLeR" type="revolute">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="ServoHolderR"/>
    <child link="UpperLegR"/>
    <axis xyz="0 0 1"/>
    <limit lower="0" upper="${2*pi}" velocity="100" effort="1"/>
  </joint>

  
  <link name="UpperLegL">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://../../STL/UpperLegL.stl" scale="0.01"/>
      </geometry>
    </visual>
    
    <collision>
        <geometry>
            <mesh filename="package://../../STL/UpperLegL.stl" scale="0.01"/>
        </geometry>
    </collision>
  </link>

  
  <joint name="revoluteUpLegLeft" type="revolute">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="servoHolderLeft"/>
    <child link="UpperLegL"/>
    <axis xyz="0 0 1"/>
    <limit lower="0" upper="${2*pi}" velocity="100" effort="1"/>
  </joint>

  
  <link name="LowerLegR">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://../../STL/LowerLegR.stl" scale="0.01"/>
      </geometry>
    </visual>
    
    <collision>
        <geometry>
            <mesh filename="package://../../STL/LowerLegR.stl" scale="0.01"/>
        </geometry>
    </collision>
  </link>

 
  <joint name="revoluteLowLegRight" type="revolute">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="UpperLegR"/>
    <child link="LowerLegR"/>
    <axis xyz="0 0 1"/>
    <limit lower="0" upper="${2*pi}" velocity="100" effort="1"/>
  </joint>

  
  <link name="LowerLegL">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://../../STL/LowerLegL.stl" scale="0.01"/>
      </geometry>
    </visual>
    
    <collision>
        <geometry>
            <mesh filename="package://../../STL/LowerLegL.stl" scale="0.01"/>
        </geometry>
    </collision>
  </link>

  
  <joint name="revoluteLowLegLeft" type="revolute">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="UpperLegL"/>
    <child link="LowerLegL"/>
    <axis xyz="0 0 1"/>
    <limit lower="0" upper="${2*pi}" velocity="100" effort="1"/>
  </joint>



</robot>